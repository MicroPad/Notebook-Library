{
	"name": "upad-parse",
	"version": "6.2.0",
	"description": "This is a parser for the NPX file format that µPad uses",
	"main": "dist/index.js",
	"scripts": {
		"build": "rm -rf ./dist; npx tsc; npx browserify -s NPXParser dist/index.js -o dist/upad-parse.browser.js",
		"test": "TZ=NZ npx jest",
		"docs": "npx typedoc --out docs/ --mode modules --excludePrivate --ignoreCompilerErrors"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/MicroPad/Web-Parser.git"
	},
	"author": "Nick Webster",
	"license": "MPL-2.0",
	"bugs": {
		"url": "https://github.com/MicroPad/Web-Parser/issues"
	},
	"homepage": "https://getmicropad.com",
	"keywords": [
		"uPad",
		"µPad",
		"parse",
		"npx",
		"parser"
	],
	"devDependencies": {
		"@types/date-fns": "^2.6.0",
		"@types/jest": "^24.0.15",
		"@types/json-stringify-safe": "^5.0.0",
		"@types/lz-string": "^1.3.33",
		"@types/scrypt-js": "^2.0.3",
		"@types/xml2js": "^0.4.4",
		"browserify": "^16.2.3",
		"jest": "^24.8.0",
		"ts-jest": "^24.0.2",
		"typedoc": "^0.14.2",
		"typescript": "^3.5.2"
	},
	"jest": {
		"rootDir": "./src",
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "((\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		],
		"testURL": "http://localhost"
	},
	"dependencies": {
		"aes-js": "^3.1.2",
		"date-fns": "^1.30.1",
		"json-stringify-safe": "^5.0.1",
		"lz-string": "^1.4.4",
		"pretty-data": "^0.40.0",
		"scrypt-js": "^2.0.4",
		"turndown": "^4.0.2",
		"turndown-plugin-gfm": "^1.0.2",
		"xml2js": "^0.4.19"
	}
}
